{
  "name": "storybook-addon-tag-badges",
  "version": "0.0.0-semantically-released",
  "description": "Display Storybook tags as badges in the sidebar and toolbar.",
  "keywords": [
    "storybook-addon",
    "storybook",
    "badges",
    "tags",
    "organize"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sidnioulz/storybook-addon-tag-badges.git"
  },
  "type": "module",
  "author": "Steve Dodier-Lazaro <sidnioulz@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./manager-helpers": "./dist/manager-helpers.js",
    "./manager": "./dist/manager.js",
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "manager.js",
    "preview.js"
  ],
  "bundler": {
    "managerEntries": [
      "src/manager.tsx",
      "src/manager-helpers.ts"
    ],
    "nodeEntries": [],
    "previewEntries": [
      "src/preview.ts",
      "src/index.ts"
    ]
  },
  "scripts": {
    "build": "tsup",
    "build:storybook": "storybook build",
    "build:watch": "pnpm build --watch",
    "clean": "rimraf ./dist",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "pack": "pnpm pack --out storybook-addon-tag-badges-$(date +%s).tgz",
    "prebuild": "pnpm clean",
    "prepare": "husky",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "pnpm build && pnpm semantic-release",
    "release:canary": "pnpm build && auto canary",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/icons": "^2.0.1",
    "@storybook/react-vite": "^10.1.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser": "4.0.18",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-istanbul": "4.0.18",
    "auto": "^11.3.6",
    "boxen": "^8.0.1",
    "chromatic": "^13.3.5",
    "commitlint": "^20.3.1",
    "dedent": "^1.7.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.58.0",
    "prettier": "^3.8.1",
    "prompts": "^2.4.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.2",
    "storybook": "^10.1.11",
    "tosource": "2.0.0-alpha.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "zx": "^8.8.5"
  },
  "peerDependencies": {
    "storybook": "^10.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=20"
  },
  "storybook": {
    "displayName": "Tag Badges",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://raw.githubusercontent.com/Sidnioulz/storybook-addon-tag-badges/main/static/icon.png"
  },
  "pnpm": {
    "overrides": {
      "vite@>=7.1.0 <=7.1.10": ">=7.1.11",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "diff@<8.0.3": ">=8.0.3",
      "undici@<6.23.0": ">=6.23.0",
      "undici@>=7.0.0 <7.18.2": ">=7.18.2",
      "@octokit/request-error@>=1.0.0 <5.1.1": ">=5.1.1",
      "@octokit/plugin-paginate-rest@>=1.0.0 <9.2.2": ">=9.2.2",
      "@octokit/request@>=1.0.0 <8.4.1": ">=8.4.1",
      "lodash-es@>=4.0.0 <=4.17.22": ">=4.17.23",
      "@isaacs/brace-expansion@<=5.0.0": ">=5.0.1"
    }
  },
  "dependencies": {
    "memoizerific": "^1.11.3"
  }
}
