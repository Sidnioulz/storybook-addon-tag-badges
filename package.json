{
  "name": "storybook-addon-tag-badges",
  "version": "0.0.0",
  "description": "Display Storybook tags as badges in the sidebar and toolbar.",
  "keywords": [
    "storybook-addons",
    "badges",
    "tags"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sidnioulz/storybook-addon-tag-badges.git"
  },
  "type": "module",
  "author": "Steve Dodier-Lazaro <sidnioulz@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ]
  },
  "scripts": {
    "build-storybook": "storybook build",
    "clean": "rimraf ./dist",
    "build": "tsup",
    "build:watch": "pnpm build --watch",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "pack:local": "pnpm pack --out storybook-addon-tag-badges-$(date +%s).tgz",
    "prebuild": "pnpm clean",
    "prepack:local": "pnpm build",
    "prepare": "husky install",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "start": "run-p build:watch \"storybook --quiet\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "resolutions": {
    "cookie": "^0.7.1",
    "micromatch": "^4.0.8"
  },
  "dependencies": {
    "@storybook/icons": "^1.2.12"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.12.0",
    "@storybook/addon-essentials": "^8.4.0-alpha.7",
    "@storybook/addon-interactions": "^8.4.0-alpha.7",
    "@storybook/addon-links": "^8.4.0-alpha.7",
    "@storybook/builder-vite": "^8.4.0-alpha.7",
    "@storybook/components": "^8.4.0-alpha.7",
    "@storybook/manager": "^8.4.0-alpha.7",
    "@storybook/manager-api": "^8.4.0-alpha.7",
    "@storybook/preview-api": "^8.4.0-alpha.7",
    "@storybook/react": "^8.4.0-alpha.7",
    "@storybook/react-vite": "^8.4.0-alpha.7",
    "@storybook/test": "^8.4.0-alpha.7",
    "@storybook/theming": "^8.4.0-alpha.7",
    "@storybook/types": "^8.4.0-alpha.7",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-istanbul": "2.1.3",
    "@vitest/coverage-v8": "2.1.3",
    "auto": "^11.2.1",
    "boxen": "^8.0.1",
    "commitlint": "^19.5.0",
    "dedent": "^1.5.3",
    "eslint": "^9.12.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "node^22": "link:@types/node^22",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "storybook": "^8.4.0-alpha.7",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "zx": "^8.1.9"
  },
  "peerDependencies": {
    "storybook": "^8.4.0-alpha.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=20"
  },
  "storybook": {
    "displayName": "Tag Badges",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
